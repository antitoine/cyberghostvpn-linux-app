{
  "appId": "fr.ancha.cyberghostvpn-linux-app",
  "productName": "CyberGhost VPN",
  "directories": {
    "output": "release/"
  },
  "files": [
    "**/*",
    "!**/*.ts",
    "!*.code-workspace",
    "!LICENSE.md",
    "!package.json",
    "!package-lock.json",
    "!src/",
    "!e2e/",
    "!hooks/",
    "!angular.json",
    "!_config.yml",
    "!karma.conf.js",
    "!tsconfig.json",
    "!tslint.json"
  ],
  "mac": {
    "icon": "dist/assets/icons/favicon.icns",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist/assets/icons/favicon.png",
    "target": [
      "AppImage",
      "snap",
      "deb",
      "zip",
      "tar.gz"
    ]
  }
}
